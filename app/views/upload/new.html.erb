
<% provide(:title, 'Upload Texture') %>

<div id='upload_texture'>
  <h1>Upload Texture</h1>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
	<%= form_for(:upload_image, url: upload_path) do |f| %>

	  <%= f.label :address %>
      <%= f.text_field :address, class: 'form-control' %>

	  <%= f.label :image, 'Select Image' %><br>
	  <%= f.file_field :image, class: 'form-control' %><br>

	  <%= f.submit 'Upload Texture', class: "btn btn-primary" %>
	<% end %>
  </div>
</div>

